{
  "name": "fitness-tracker-mvp",
  "version": "1.0.0",
  "description": "A web application for tracking fitness goals.",
  "main": "index.js",
  "scripts": {
    "start": "node api/index.js",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
     "dev:api": "nodemon api/index.js",
    "dev:client": "vite",
    "build": "vite build",
    "preview": "vite preview",
     "lint": "eslint src/**/*.jsx",
    "lint:fix": "eslint src/**/*.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css}\"",
    "test": "jest"
  },
  "keywords": [
    "fitness",
    "tracker",
    "goals",
    "react",
    "node.js",
    "mongodb"
  ],
  "author": "MVP Developer",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
     "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mongoose": "^8.9.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.4.2",
    "vite": "^6.0.6"
  }
}